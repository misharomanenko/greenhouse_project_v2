#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules/@builder.io/partytown/bin/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules/@builder.io/partytown/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules/@builder.io/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules/@builder.io/partytown/bin/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules/@builder.io/partytown/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules/@builder.io/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/@builder.io+partytown@0.10.2/node_modules:/Users/misharomanenko/Documents/Code/paraform/greenhouse_will/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@builder.io/partytown/bin/partytown.cjs" "$@"
else
  exec node  "$basedir/../@builder.io/partytown/bin/partytown.cjs" "$@"
fi
